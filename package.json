{
  "name": "paintpad",
  "version": "2.0.1",
  "description": "Canvas component which allows freeform painting",
  "keywords": [
    "paintpad",
    "canvas",
    "ui",
    "draw",
    "drawing",
    "sketch",
    "paint",
    "painting"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xiadevisser/paintpad.git"
  },
  "homepage": "https://github.com/xiadevisser/paintpad#readme",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "webpack --mode development -w",
    "build": "rimraf dist && webpack --mode production",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "pack": "npm run build && npm pack",
    "deploy": "gh-pages -d dist"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "dts-bundle": "^0.7.3",
    "dts-bundle-webpack": "^1.0.2",
    "eslint": "^8.4.0",
    "gh-pages": "^3.2.3",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1"
  }
}
